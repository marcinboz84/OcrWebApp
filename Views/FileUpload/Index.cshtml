<form method="post" enctype="multipart/form-data" asp-controller="FileUpload" asp-action="Index">
    <div class="form-group">
        <div class="col-md-10">
            <p>Wczytaj plik:</p>
            <input type="file" name="file" />
        </div>
    </div>

    <div class="form-group">
        <div class="col-md-10">
            <input type="submit" value="Wczytaj" />
        </div>     
    </div>

    @if(ViewBag.Result)
    {
        <fieldset>
            <div class="form-group">
                <label>Wynik:</label>
                <textarea class="form-control" readonly rows="10">@ViewBag.res</textarea>
            </div>
        </fieldset>
    }
</form>